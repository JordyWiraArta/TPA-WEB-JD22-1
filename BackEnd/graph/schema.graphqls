# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
  email: String!
  password: String!

}

type Query {
  users: [User!]!
}

input newUser{
  id: ID!
  name: String!
  email: String!
  password: String!
}

type Mutation {
  createUser(input: newUser!): User!
}
